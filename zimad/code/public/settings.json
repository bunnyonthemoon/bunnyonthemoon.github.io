{
	"default": "+",
	"functions": {
		"+": {
			"name": "+",
			"show": true,
			"function": "$1 + $2",
			"error": null
		},
		"-": {
			"name": "-",
			"show": true,
			"function": "$1 - $2",
			"error": null
		},
		"*": {
			"name": "*",
			"show": true,
			"function": "$1 * $2",
			"error": [
				{
					"target": "any",
					"value": "0",
					"alertText": "Умножение на ноль всегда даёт ноль",
					"calc": true
				}
			]
		},
		"/": {
			"name": "/",
			"show": true,
			"function": "$1 / $2",
			"error": [
				{
					"target": "$2",
					"value": "0",
					"alertText": "Нельзя делить на ноль",
					"calc": false
				}
			]
		}
	}
}