// Этот файл является лишь инструкцией, исполняемый файл называется settings.json
{
	// Выбор операции по умолчанию. Принимает в значение ключ одной из функций
	"default": "/",
	"functions": {
		"/": {
			// Выводимое название операции
			"name": "/",
			// Флажок показать/скрыть
			"show": true,
			// Код операции, где $1 = первый инпат и $2 = второй инпат
			"function": "$1 / $2",
			// Вывод ошибок. Принимает массив возможных ошибок. Если таковых нет, поле можно не указывать
			"error": [
				{
					// Отслеживаемое значение. Принимает значения $1 - первое значение, $2 - второе, 'any' - любой из инпатов, 'both' - оба инпата
					"target": "$2",
					// Значение, при котором выводится ошибка
					"value": "0",
					// Текст алерта
					"alertText": "Нельзя делить на ноль",
					// Продолжать ли вычисление выражения
					"calc": false
				}
			]
		}
	}
}